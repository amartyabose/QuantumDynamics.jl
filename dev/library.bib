@article{cookWhichAlgorithmBest2023,
  title = {Which {{Algorithm Best Propagates}} the {{Meyer}}--{{Miller}}--{{Stock}}--{{Thoss Mapping Hamiltonian}} for {{Non-Adiabatic Dynamics}}?},
