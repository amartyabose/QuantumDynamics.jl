@article{hashemiAssessmentInitioBethe2021,
  title = {Assessment of the {{Ab Initio Bethe}}--{{Salpeter Equation Approach}} for the {{Low-Lying Excitation Energies}} of {{Bacteriochlorophylls}} and {{Chlorophylls}}},
