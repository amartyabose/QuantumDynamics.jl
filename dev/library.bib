@article{heapsAccurateNonadiabaticQuantum2016,
  title = {Accurate Non-Adiabatic Quantum Dynamics from Pseudospectral Sampling of Time-Dependent {{Gaussian}} Basis Sets},
  author = {Heaps, Charles W. and Mazziotti, David A.},
  year = {2016},
  month = aug,
  journal = {The Journal of Chemical Physics},
  volume = {145},
