@article{pavosevicMulticomponentQuantumChemistry2020,
  title = {Multicomponent {{Quantum Chemistry}}: {{Integrating Electronic}} and {{Nuclear Quantum Effects}} via the {{Nuclear}}--{{Electronic Orbital Method}}},
  shorttitle = {Multicomponent {{Quantum Chemistry}}},
  author = {Pavo{\v s}evi{\'c}, Fabijan and Culpitt, Tanner and {Hammes-Schiffer}, Sharon},
  year = {2020},
  month = may,
  journal = {Chemical Reviews},
  volume = {120},
