@article{hildebrandtEnergyTransferSemiconductor2017,
  title = {Energy Transfer with Semiconductor Quantum Dot Bioconjugates: {{A}} Versatile Platform for Biosensing, Energy Harvesting, and Other Developing Applications},
  author = {Hildebrandt, Niko and Spillmann, Christopher M. and Algar, W. Russ and Pons, Thomas and Stewart, Michael H. and Oh, Eunkeu and Susumu, Kimihiro and D{\'i}az, Sebastian A. and Delehanty, James B. and Medintz, Igor L.},
  year = {2017},
