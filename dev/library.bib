  file = {/home/amartya/my_drive/ZoteroFiles/journalArticle/2017/Makri - 2017 - Iterative blip-summed path integral for quantum dynamics in strongly dissipative environments/Makri - 2017 - Iterative blip-summed path integral for quantum dynamics in strongly dissipative environments.pdf}
}

