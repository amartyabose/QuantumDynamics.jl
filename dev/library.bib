@article{ritzKeldyshQFTCodebaseRealfrequency2024,
  title = {{{KeldyshQFT}}: {{A C}}++ Codebase for Real-Frequency Multiloop Functional Renormalization Group and Parquet Computations of the Single-Impurity {{Anderson}} Model},
  author = {Ritz, Nepomuk and Ge, Anxiang and Walter, Elias and Aguirre, Santiago and {von Delft}, Jan and Kugler, Fabian B.},
