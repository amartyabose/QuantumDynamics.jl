@article{zhongEfficientTimedomainImplementation2023,
  title = {An Efficient Time-Domain Implementation of the Multichromophoric {{F{\"o}rster}} Resonant Energy Transfer Method},
  author = {Zhong, Kai and Nguyen, Hoang Long and Do, Thanh Nhut and Tan, Howe-Siang and Knoester, Jasper and Jansen, Thomas L. C.},
