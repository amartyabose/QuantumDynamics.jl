@article{songFirstPrinciplesNonadiabatic2020,
  title = {First {{Principles Nonadiabatic Excited-State Molecular Dynamics}} in {{NWChem}}},
  author = {Song, Huajing and Fischer, Sean A. and Zhang, Yu and Cramer, Christopher J. and Mukamel, Shaul and Govind, Niranjan and Tretiak, Sergei},
  year = {2020},
  month = oct,
  journal = {Journal of Chemical Theory and Computation},
  volume = {16},
