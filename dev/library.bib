@article{gustinMappingElectronicDecoherence2023,
  title = {Mapping Electronic Decoherence Pathways in Molecules},
  author = {Gustin, Ignacio and Kim, Chang Woo and McCamant, David W. and Franco, Ignacio},
  year = {2023},
  month = dec,
  journal = {Proceedings of the National Academy of Sciences},
