  file = {/home/amartya/my_drive/ZoteroFiles/journalArticle/2014/Makri - 2014 - Blip decomposition of the path integral Exponential acceleration of real-time calculations on quant/Makri - 2014 - Blip decomposition of the path integral Exponential acceleration of real-time calculations on quant.pdf}
}

