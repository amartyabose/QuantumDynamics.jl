  title = {Singlet {{Fission Rate}}: {{Optimized Packing}} of a {{Molecular Pair}}. {{Ethylene}} as a {{Model}}},
  author = {Zaykov, Alexandr and Felkel, Petr and Buchanan, Eric A. and Jovanovic, Milena and Havenith, Remco W. A. and Kathir, R. K. and Broer, Ria and Havlas, Zden{\v e}k and Michl, Josef},
