@article{huaiTheoreticalStudySpectral2021,
  title = {Theoretical {{Study}} of the {{Spectral Differences}} of the {{Fenna}}--{{Matthews}}--{{Olson Protein}} from {{Different Species}} and {{Their Mutants}}},
  author = {Huai, Zhe and Tong, Zhengqing and Mei, Ye and Mo, Yan},
