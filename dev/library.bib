@article{daniQuantumStateStateRates2022,
  title = {Quantum {{State-to-State Rates}} for {{Multistate Processes}} from {{Coherences}}},
  author = {Dani, Reshmi and Makri, Nancy},
  year = {2022},
  month = sep,
