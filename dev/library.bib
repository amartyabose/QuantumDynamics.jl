@article{mandalTheoreticalAdvancesPolariton2023,
  title = {Theoretical {{Advances}} in {{Polariton Chemistry}} and {{Molecular Cavity Quantum Electrodynamics}}},
  author = {Mandal, Arkajit and Taylor, Michael A.D. and Weight, Braden M. and Koessler, Eric R. and Li, Xinyang and Huo, Pengfei},
  year = {2023},
