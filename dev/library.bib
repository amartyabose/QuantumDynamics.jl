@article{schuttSchNetDeepLearning2018,
  title = {{{SchNet}} -- {{A}} Deep Learning Architecture for Molecules and Materials},
  author = {Sch{\"u}tt, K. T. and Sauceda, H. E. and Kindermans, P.-J. and Tkatchenko, A. and M{\"u}ller, K.-R.},
