@article{kunduFranckCondonHerzberg2022,
  title = {Franck--{{Condon}} and {{Herzberg}}--{{Teller Signatures}} in {{Molecular Absorption}} and {{Emission Spectra}}},
  author = {Kundu, Sohang and Roy, Partha Pratim and Fleming, Graham R. and Makri, Nancy},
