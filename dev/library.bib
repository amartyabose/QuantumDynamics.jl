  file = {/home/amartya/my_drive/ZoteroFiles/journalArticle/2023/Bose - 2023 - QuantumDynamics.jl A modular approach to simulations of dynamics of open quantum systems/Bose - 2023 - QuantumDynamics.jl A modular approach to simulations of dynamics of open quantum systems.pdf}
