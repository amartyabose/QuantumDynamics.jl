@article{zhengUltrafastNonadiabaticDynamics2016,
  title = {Ultrafast Nonadiabatic Dynamics of Singlet Fission: {{Quantum}} Dynamics with the Multilayer Multiconfigurational Time-Dependent Hartree ({{ML-MCTDH}}) Method},
