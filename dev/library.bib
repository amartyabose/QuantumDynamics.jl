  title = {Phonon-Mediated Quantum State Transfer and Remote Qubit Entanglement},
  author = {Bienfait, A. and Satzinger, K. J. and Zhong, Y. P. and Chang, H.-S. and Chou, M.-H. and Conner, C. R. and Dumur, {\'E}. and Grebel, J. and Peairs, G. A. and Povey, R. G. and {A. N. Cleland}},
