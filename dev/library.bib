@article{ibeleComparingStochasticselectionInitio2021,
  title = {Comparing (Stochastic-Selection) Ab Initio Multiple Spawning with Trajectory Surface Hopping for the Photodynamics of Cyclopropanone, Fulvene, and Dithiane},
  author = {Ibele, Lea M. and Lassmann, Yorick and Mart{\'i}nez, Todd J. and Curchod, Basile F. E.},
